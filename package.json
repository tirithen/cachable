  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tirithen/cachable/issues"
  },
  "homepage": "https://github.com/tirithen/cachable#readme",
  "dependencies": {
    "mkdirp": "^0.5.1",
    "rmdir": "^1.2.0"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2"
  }
}
